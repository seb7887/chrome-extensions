{
  "name": "Websocket",
  "description": "Websocket example",
  "version": "1.0",
  "manifest_version": 2,
  "author": "Sebastian Matias Segura",
  "browser_action": {
    "default_icon": "images/icon_blue.png",
    "default_title": "Websocket.io"
  },
  "permissions": ["notifications"],
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "html/options.html",
  "icons": {
    "16": "images/icon_blue.png",
    "32": "images/icon_blue.png",
    "48": "images/icon_blue.png",
    "128": "images/icon_blue.png"
  }
}
