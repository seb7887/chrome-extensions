{
  "manifest_version": 2,
  "short_name": "Another Tabs",
  "name": "Another Tabs",
  "description": "Another Chrome Extension",
  "version": "1.0",
  "permissions": ["bookmarks", "chrome://favicon/"],
  "browser_action": {
    "default_title": "Another Tab",
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'; img-src chrome://favicon;"
}
